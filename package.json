{
  "name": "teletypes",
  "version": "1.1.0",
  "description": "Half-machine parsed types for the Telegram Bot API",
  "main": "./lib/index.js",
  "exports": {
    ".": "./lib/index.js",
    "./types": "./lib/types.js",
    "./methods": "./lib.methods.js"
  },
  "scripts": {
    "dump": "node scripts/dump.js",
    "prepublishOnly": "npm run dump && npm run test",
    "publish:linux": "echo //registry.npmjs.org/:_authToken=${NPM_TOKEN} > .npmrc",
    "postpublish:linux": "rm .npmrc",
    "test": "tsc test/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teletypes/js.git"
  },
  "keywords": [
    "telegram",
    "teletypes",
    "telegram-bots",
    "telegram-bot",
    "bot",
    "types",
    "typings",
    "api"
  ],
  "author": "Teselka",
  "types": "./lib/index.d.ts",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/teletypes/js/issues"
  },
  "homepage": "https://github.com/teletypes/js#readme",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "devDependencies": {
    "libgovno": "^1.2.0",
    "node-html-parser": "^5.3.3",
    "run-script-os": "^1.1.6"
  }
}
